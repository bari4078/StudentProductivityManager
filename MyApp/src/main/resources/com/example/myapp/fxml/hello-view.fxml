<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.myapp.controllers.QuizController">
    <children>
        <AnchorPane prefHeight="600.0" prefWidth="800.0">
            <children>
                <ImageView fitHeight="600.0" fitWidth="800.0" layoutY="-14.0">
                    <image>
                        <Image url="@backhomepage.jpg" />
                    </image>
                </ImageView>

                <Button fx:id="Next" layoutX="87.0" layoutY="309.0" mnemonicParsing="false" onAction="#startQuiz" prefHeight="99.0" prefWidth="161.0" styleClass="wooden-button" stylesheets="@style2.css" text="Start Test" />
            <Button layoutX="688.0" layoutY="542.0" mnemonicParsing="false" onAction="#goToHomePage" prefHeight="43.0" prefWidth="79.0" styleClass="stone-button" stylesheets="@style3.css" text="Back">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
            </children>
        </AnchorPane>
    </children>
</StackPane>
