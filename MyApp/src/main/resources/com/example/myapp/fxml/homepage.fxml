<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane fx:id="rootPane" prefHeight="600" prefWidth="800" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.myapp.controllers.HomePageController">
    <children>

        <HBox fx:id="navBar" alignment="CENTER" spacing="40" styleClass="nav-bar" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
            <Button fx:id="flashcardButton" onAction="#goToFlashcards" prefHeight="50.0" prefWidth="180.0" styleClass="big-button" text="Flashcards" />
            <Button fx:id="timerButton" onAction="#goToTimer" prefHeight="50" prefWidth="180" styleClass="big-button" text="Timer" />
        </HBox>
        <Button fx:id="Quizbutton" layoutX="502.0" layoutY="466.0" onAction="#goToQuiz" prefHeight="50.0" prefWidth="180.0" styleClass="big-button" text="Mental Health Test" />
        <Button fx:id="todoButton" layoutX="110.0" layoutY="457.0" onAction="#goToTodo" prefHeight="50.0" prefWidth="180.0" styleClass="big-button" text="TODO List" />
    </children>
</AnchorPane>
